	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="fsm" class="com.sxj.statemachine.StateMachineFactory" factory-method="newInstance">
		<constructor-arg>
			<bean id="fsmConfig" class="com.sxj.statemachine.StateMachineConfig"></bean>
		</constructor-arg>
	</bean>
</beans>
